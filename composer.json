{
    "name": "unialteri/states",
    "version": "0.9-beta",
    "type": "library",
    "description": "Uni Alteri is a lib to allow developers to write states for their business object with PHP 5.4+",
    "keywords": ["states","class"],
    "time": "2014-08-27",
    "homepage": "http://teknoo.it/states",
    "license": ["MIT","GPL-3.0+"],
    "authors": [
        {
            "name": "Richard DÃ©loge",
            "email": "r.deloge@uni-alteri.com",
            "role":"Software architect, Developer"
        }
    ],
    "support": {
